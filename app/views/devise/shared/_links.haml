- if controller_name != 'sessions'
  = link_to t(".sign_in", :default => "Sign in"), new_session_path(resource_name), class: 'btn btn-default'
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to t(".sign_up", :default => "Sign up"), new_registration_path(resource_name), class: 'btn btn-default'
  %br/