%table.table
  %tr
    %th E-mail
    %td= user.email
  %tr
    %th Password
    %td{ style: "font-style: italic;" } hidden
  %tr
    %th Account since
    %td= user.created_at
  %tr
    %th Logged in
    %td= "#{user.sign_in_count} times"
  %tr
    %th Current log in
    %td= user.current_sign_in_at
  %tr
    %th Last logged in
    %td= user.last_sign_in_at